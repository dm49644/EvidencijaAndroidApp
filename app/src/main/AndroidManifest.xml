<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.fer.ruazosa.evidencijaopremeulaboratoriju">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ItemsActivity" />
        <activity android:name=".UsersActivity" />
        <activity android:name=".DetailedItemActivity" />
        <activity android:name=".AddItemActivity" />

        <service
            android:name=".urlConnection.URLService"
            android:exported="false" />

        <activity android:name=".PreviousBorrowsActivity" />
        <activity android:name=".ReserveActivity"></activity>
    </application>

</manifest>